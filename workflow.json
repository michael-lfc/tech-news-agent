{
  "id": "tech-news-workflow",
  "name": "Tech News Agent Workflow",
  "short_description": "Fetches and delivers latest technology news headlines",
  "description": "Handles incoming Telex messages and responds with tech news from external API",
  "nodes": [
    {
      "id": "telex-trigger",
      "type": "trigger",
      "label": "Telex Message Trigger",
      "config": {
        "event": "message",
        "platform": "telex"
      },
      "position": [100, 100]
    },
    {
      "id": "message-processor",
      "type": "processor",
      "label": "Process Incoming Message",
      "config": {
        "handler": "parseTelexMessage"
      },
      "position": [350, 100]
    },
    {
      "id": "tech-news-agent",
      "type": "agent",
      "label": "Tech News Agent",
      "config": {
        "agentId": "tech_news_agent"
      },
      "position": [600, 100]
    },
    {
      "id": "response-formatter",
      "type": "processor", 
      "label": "Format Telex Response",
      "config": {
        "handler": "formatTelexResponse"
      },
      "position": [850, 100]
    },
    {
      "id": "send-response",
      "type": "respond",
      "label": "Send Response to Telex",
      "config": {
        "platform": "telex"
      },
      "position": [1100, 100]
    }
  ],
  "edges": [
    {
      "id": "trigger-to-processor",
      "from": "telex-trigger",
      "to": "message-processor"
    },
    {
      "id": "processor-to-agent",
      "from": "message-processor", 
      "to": "tech-news-agent"
    },
    {
      "id": "agent-to-formatter",
      "from": "tech-news-agent",
      "to": "response-formatter"
    },
    {
      "id": "formatter-to-respond",
      "from": "response-formatter",
      "to": "send-response"
    }
  ]
}