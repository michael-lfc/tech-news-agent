{
  "id": "tech-news-workflow",
  "name": "Tech News Fetcher",
  "description": "Fetches technology news from NewsAPI",
  "nodes": [
    {
      "id": "trigger",
      "type": "trigger",
      "config": {
        "event": "message",
        "platform": "telex"
      },
      "position": [100, 100]
    },
    {
      "id": "agent",
      "type": "agent",
      "config": {
        "agentId": "techpulse"
      },
      "position": [300, 100]
    }
  ],
  "edges": [
    {
      "from": "trigger",
      "to": "agent"
    }
  ]
}