    {
      "id": "agent",
      "name": "Tech News Agent",
      "type": "agent",
      "typeVersion": 1,
      "position": [1050, 112],
      "config": {
        "agentId": "techpulse",
        "url": "https://powerful-atoll-01260-84ad49c653d5.herokuapp.com/telex/a2a/agent/techpulse"
      }
    }
  ],
  "edges": [
    {
      "from": "trigger",
      "to": "agent"
    }
  ],
