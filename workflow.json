{
  "id": "tech-news-workflow",
  "name": "Tech News Fetcher",
  "description": "Fetches technology news from NewsAPI when users ask about news",
  "category": "technology",
  "short_description": "Fetches latest tech news headlines",
  "long_description": "Handles incoming Telex messages and responds with technology news from NewsAPI",
  "nodes": [
    {
      "id": "trigger",
      "type": "trigger",
      "label": "Message Trigger",
      "config": {
        "event": "message",
        "platform": "telex",
        "conditions": [
          {
            "field": "text",
            "operator": "contains", 
            "value": "news"
          }
        ]
      },
      "position": [100, 100]
    },
    {
      "id": "agent",
      "type": "agent",
      "label": "Tech News Agent",
      "config": {
        "agentId": "techpulse"
      },
      "position": [300, 100]
    }
  ],
  "edges": [
    {
      "id": "trigger-to-agent",
      "from": "trigger",
      "to": "agent"
    }
  ]
}