{
  "id": "tech-news-workflow",
  "name": "Tech News Workflow",
  "description": "Fetches technology news from NewsAPI",
  "nodes": [
    {
      "id": "trigger",
      "name": "Message Trigger",
      "type": "trigger",
      "config": {
        "event": "message",
        "platform": "telex",
        "conditions": [
          {
            "field": "text",
            "value": "news", 
            "operator": "contains"
          }
        ]
      },
      "position": [100, 100]
    },
    {
      "id": "agent",
      "name": "Tech News Agent",
      "type": "agent",
      "config": {
        "agentId": "019a5969-2cde-74b0-b10b-6bf7656e9469"
      },
      "position": [300, 100]
    }
  ],
  "edges": [
    {
      "from": "trigger",
      "to": "agent"
    }
  ]
}